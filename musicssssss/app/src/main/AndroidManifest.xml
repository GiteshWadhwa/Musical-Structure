<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.wadhwa.gitesh.secondproject">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity"
            android:label="Music Player">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".albums"
            android:label="NoW Playing"/>
        <activity android:name=".artist"
            android:label="Artist"/>
        <activity android:name=".folders"
            android:label="Folders"/>
        <activity android:name=".paymentActivity"
            android:label="GooglePay"/>
        <activity android:name=".songs"
            android:label="Songs"></activity>
    </application>

</manifest>